#include<iostream>
using namespace std;

void search(int arr[], int n, int key){
     for(int i = 0; i<n; i++){
         if(arr[i] == key){
             cout<<"key is present " <<" " <<key<<endl;
         }else{
             cout<<"key is not present " << " " <<key<<endl;
         }
     }
}

int main(){

    int n;
    cout<<"Enter the size of array"<<endl;
    cin>>n;

    int arr[n];
    for(int i = 0; i<n; i++){
        cout<<"Enter the elements in array"<<endl;
        cin >> arr[i];
    }

    int key;
    cout<<"Enter the key you want to serach in array"<<endl;
    cin>>key;

  search(arr,n,key);
}
